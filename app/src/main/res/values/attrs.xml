<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--菜单，左侧图片+文字，右侧开关按钮-->
    <declare-styleable name="CheckButtonView">
        <attr name="is_show_left_img" format="boolean"/>
        <attr name="left_txt_name" format="string"/>
        <attr name="left_img_resource" format="reference"/>
        <!--右侧显示图片-->
        <attr name="right_image" format="integer"/>
        <attr name="right_back_img" format="boolean"/>
    </declare-styleable>

</resources>